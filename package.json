{
  "name": "velox2",
  "version": "1.0.23",
  "description": "Velox chat app",
  "main": "run.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --win --x64",
    "publish": "electron-builder --win --x64 --publish=always",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Hyztax",
  "license": "MIT",
  "devDependencies": {
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "electron-updater": "^6.6.2"
  },
  "build": {
    "appId": "com.hyztax.velox",
    "productName": "Velox",
    "win": {
      "target": "nsis",
      "icon": "icon.ico",
      "artifactName": "Velox-Setup.exe"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "hyztax",
        "repo": "Velox"
      }
    ]
  }
}
